PREFIX coxpresdb: <http://coxpresdb.jp/rdf/def/0.1/>

# Node mappings
(gene:Gene)
    <http://coxpresdb.jp/rdf/db/Sce.v12> coxpresdb:gene_id ?gene .

# Edge mappings
(gene1:Gene)-[:coexpress {mutual_rank:mutual_rank, pcc:pcc}]-(gene2:Gene)
    ?co coxpresdb:gene_id_1 ?gene1 ;
        coxpresdb:gene_id_2 ?gene2 ;
        coxpresdb:mutual_rank ?mutual_rank ;
        coxpresdb:pcc ?pcc .
    FILTER(?pcc >= 0.85)
